import java.util.Scanner;

public class pratica9 {

    public static void main(String[] args) {
        Scanner ler = new Scanner(System.in);
        String nome;
        
        System.out.print("Informe seu nome: ");
        nome = ler.nextLine();
        
        for(int i=1; i<=10; i++) {
            System.out.println("O seu nome Ã© " + nome);
        }
        
        ler.close();
    }
}
